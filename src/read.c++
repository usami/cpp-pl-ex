#include <iostream>
using namespace std;

int main()
{
  cout << "bool?\n";

  bool b = 0;
  cin >> b;

  cout << "bool: " << b << '\n';

  cout << "char?\n";

  char c = 0;
  cin >> c;

  cout << "char: " << c << endl;

  cout << "int?\n";

  int i = 0;
  cin >> i;

  cout << "int: " << i << endl;

  cout << "double?\n";

  double d = 0;
  cin >> d;

  cout << "double: " << d << endl;

  cout << "string?\n";

  string s = "";
  cin >> s;

  cout << "string: " << s << endl;
}
